group qicvgwalker;
line(id,x1,y1,x2,y2,style) ::= "\<line id=\"<id>\" x1=\"<x1>\" y1=\"<y1>\" x2=\"<x2>\" y2=\"<y2>\" <style> /\>"
path(id,style) ::= "\<path id=\"<id>\" <style> /\>"
square(id,x,y,size,style) ::= "\<rect id=\"<id>\" x=\"<x>\" y=\"<y>\" width=\"<size>\" height=\"<size>\" <style> /\>"
circle(id,cx,cy,r,style) ::= "\<circle id=\"<id>\" cx=\"<cx>\" cy=\"<cy>\" r=\"<r>\" <style> /\>"
fillcolor(color) ::= "<if(color)>fill:<color>;<endif>"
strokecolor(color) ::= "<if(color)>stroke:<color>;<endif>"
stroke(width) ::= "<if(width)>stroke-width:<width>;<endif>"
styledef(color,bordercolor,width) ::= "style=\"<fillcolor(color=color)> <strokecolor(color=bordercolor)> <stroke(width=width)>\""